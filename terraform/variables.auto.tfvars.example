# project prefix/tag for all object names
projectPrefix = "user-build-sca-"
# aws region where build and resources are created
awsRegion = "us-east-1"
# instance typesize default is 4x16.
instanceType = "m4.xlarge"
# Existing aws keypair name
awsKeyName = "myAws-keyPair"
# admin source address https://www.ipchicken.com/ with cidr mask ip/32
adminSrcAddr = "myexternalip/32"
#Comma seperated list of repositories to clone
repositories = "https://github.com/f5devcentral/terraform-aws-f5-sca.git,https://github.com/f5devcentral/terraform-aws-bigip.git"
#url to onboard script to template
onboardScript = "https://raw.githubusercontent.com/vinnie357/bash-onboard-templates/master/terraform/aws/sca/onboard.sh"
